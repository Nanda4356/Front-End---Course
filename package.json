{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page for natours",
  "homepage": "https://github.com/Nanda4356/Front-End---Course#readme",
  "bugs": {
    "url": "https://github.com/Nanda4356/Front-End---Course/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nanda4356/Front-End---Course.git"
  },
  "license": "ISC",
  "author": "Nanda",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.min.css --style=compressed",
    "build:css": "npm run compile:sass && npm run concat:css && npm run prefix:css && npm run compress:css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "build": "npm run build:css"
  },
  "dependencies": {
    "braces": "^3.0.3",
    "chokidar": "^4.0.3",
    "detect-libc": "^1.0.3",
    "fill-range": "^7.1.1",
    "immutable": "^5.1.4",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "micromatch": "^4.0.8",
    "node-addon-api": "^7.1.1",
    "picomatch": "^2.3.1",
    "readdirp": "^4.1.2",
    "save-dev": "^0.0.1-security",
    "source-map-js": "^1.2.1",
    "to-regex-range": "^5.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "concat-cli": "^4.0.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.97.2"
  }
}
